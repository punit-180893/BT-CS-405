///Demonstrating the concept of (a)Default constructor (b)Parameterised constructor (c)constructor overloading/// 



#include <iostream>
using namespace std;
#include <conio.h>
class test
{int x;
int y;
public:
	void display()
	{cout<<"x is="<<x<<endl;
	cout<<"y is ="<<y<<endl;
	}
	test()
	{x=-1;
	y=-1;
	}
	test(int a)
	{x=a;
	}
	test (int a,int b)
	{x=a;
	y=b;
	}
};

int main()
{test a1(-1,-1);
a1.display();
test a2(34);
a2.display();
test a3(45,56);
a3.display();
return 0;
}
